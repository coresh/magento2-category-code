                            <resource id="Magento_Catalog::config_catalog">
                                <resource id="Okaeli_CategoryCode::config" title="Okaeli Category Code" sortOrder="20" />
                            </resource>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
